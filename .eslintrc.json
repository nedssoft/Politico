{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
      "node": true,
      "browser": true,
      "es6": true,
      "mocha": true
  },
  "rules": {
      "class-methods-use-this": 0,
      "object-curly-newline" : 0,
      "array-callback-return": 0,
      "consistent-return" : 0,
      "prefer-destructuring": 0,
      "no-console": "off"
  }

}